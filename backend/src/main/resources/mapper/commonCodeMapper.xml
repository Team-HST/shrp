<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hst.shrp.dao.CommonCodeDAO">

    <select id="findAllCodesByGrpCd" resultType="EntityCommonCode">
        SELECT GRP_CD,
               SUB_CD,
               SUB_NM,
               DESC
          FROM SHRP_COMM_CD
         WHERE GRP_CD = #{grpCd}
    </select>

</mapper>